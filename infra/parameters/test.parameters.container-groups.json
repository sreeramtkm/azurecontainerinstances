{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "replacelocationwithazurelocationname"
    },
    "appname": {
      "value": "replacewithanameforcontainerapp"
    },
    "port": {
      "value": 8501
    },
    "cpucoresforapp": {
      "value": 3
    },
    "memoryingbforapp": {
      "value": 10
    },
    "cpucoresforoauthproxy": {
      "value": 1
    },
    "memoryingbforoauthproxy": {
      "value": 1
    },
    "oauthproxyoidcissuerurl": {
      "value": "{replacewithoidcproxyissuerurl}"
    },
    "domainname": {
      "value": "{replacewithdomainname"
    },
    "oauthproxyclientid": {
      "value": "{replacewithoauthproxyclientid"
    }
  }
}
